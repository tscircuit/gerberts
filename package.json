{
  "name": "gerberts",
  "version": "0.0.1",
  "description": "Gerber file format parser and serializer for TypeScript",
  "type": "module",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tscircuit/gerberts.git"
  },
  "scripts": {
    "test": "bun test",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "typecheck": "bunx tsc --noEmit",
    "build": "tsup-node ./lib/index.ts --format esm --dts",
    "build:site": "bun build ./site/index.html --outdir site-export",
    "dev": "bun --hot ./site/index.html"
  },
  "keywords": [
    "gerber",
    "pcb",
    "parser",
    "serializer",
    "electronics"
  ],
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/bun": "latest",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "bun-match-svg": "^0.0.14",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
