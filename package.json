{
  "name": "gerberts",
  "version": "0.0.1",
  "description": "Gerber file format parser and serializer for TypeScript",
  "type": "module",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tscircuit/gerberts.git"
  },
  "scripts": {
    "test": "bun test",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "typecheck": "bunx tsc --noEmit",
    "build": "tsup-node ./lib/index.ts --format esm --dts"
  },
  "keywords": [
    "gerber",
    "pcb",
    "parser",
    "serializer",
    "electronics"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "bun-match-svg": "^0.0.14",
    "tsup": "^8.5.1"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
